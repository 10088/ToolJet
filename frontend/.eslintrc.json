{
  "env": {
    "browser": true,
    "amd": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended",
    "plugin:cypress/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "off",
    "react/prop-types": 0,
    "no-underscore-dangle": ["error", { "allow": ["_self", "_"] }],
    "max-len": 0,
    "no-bitwise": 0,
    "no-use-before-define": ["error", { "variables": false, "functions": false }],
    "no-nested-ternary": 0,
    "no-loop-func": 0,
    "import/no-unresolved": [2, { "commonjs": true }],
    "react/display-name": "off",
    "react/no-deprecated": "off",
    "no-unused-vars": [2, { "args": "after-used", "argsIgnorePattern": "reject" }]
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": "webpack"
  },
  "globals": {
    "atob": true,
    "HTMLCanvasElement": true,
    "fetch": true,
    "describe": true,
    "FormData": true,
    "expect": true,
    "it": true,
    "jest": true,
    "beforeEach": true,
    "afterEach": true,
    "localStorage": true,
    "test": true,
    "Blob": true,
    "confirm": true,
    "process": true,
    "module": true,
    "__dirname": true
  }
}
