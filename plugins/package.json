{
  "name": "@gandharv99/tooljet-plugins",
  "version": "0.0.11",
  "source": "server.ts",
  "types": "dist/server.d.ts",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "build": "tsc -b",
    "create:client": "node create-client-entry",
    "create:server": "node create-server-entry",
    "prebuild:packages": "npm run clean:packages && npm run create:client && npm run create:server",
    "build:packages": "lerna run build --stream",
    "clean:packages": "lerna run clean --parallel",
    "bootstrap": "lerna clean -y && lerna bootstrap --hoist"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "jest": "^27.4.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
